@model Ropey_DVDs.Models.CastMember

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>CastMember</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DVDNumber" />
            <input type="hidden" asp-for="ActorNumber" />
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" /> <a asp-action="Index"><i class="fa-solid fa-arrow-left"></i></a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
